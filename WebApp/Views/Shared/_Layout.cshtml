@using System.Configuration
@using WebApp.CustomHelpers
@using WebApp.Globalization.Scripts
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>@ViewBag.Title</title>
    <link href="@Url.ContentCache("~/Content/External/bootstrap.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.ContentCache("~/Content/External/widget.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.ContentCache("~/Content/External/bootstrap-datepicker.min.css")" rel="stylesheet" type="text/css"/>
    <link href="@Url.ContentCache("~/Content/External/bootstrap-timepicker.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.ContentCache("~/Content/External/jquery.bootstrap-touchspin.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.ContentCache("~/Content/External/dataTables.bootstrap.min.css")" rel="stylesheet"/>
    <link href="@Url.ContentCache("~/Content/External/responsive.bootstrap.min.css")" rel="stylesheet" />
    @RenderSection("stylesheet", required: false)
    <link href="@Url.ContentCache("~/Content/themecanvas.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.ContentCache("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <!-- jQuery -->
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/jquery.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/jquery-ui.min.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/jquery.blockUI.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/clipboard.min.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("Scripts/utils/jquery.textchange.min.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/widget.js")"></script>
    <!-- Bootstrap Core JavaScript -->
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/bootstrap.js")"></script>
    <!-- Globalization -->
    <script type="text/javascript" src="@Url.ContentCache("Scripts/globalize.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("Scripts/cultures/globalize.culture.es.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/bootstrap-datepicker.min.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/bootstrap-timepicker.js")"></script>
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/External/bootstrap-datepicker.es.min.js")"></script>
    <script src="@Url.ContentCache("~/Scripts/External/jquery.bootstrap-touchspin.min.js")"></script>
    <script src="@Url.ContentCache("~/Scripts/External/jquery.dataTables.min.js")"></script>
    <script src="@Url.ContentCache("~/Scripts/External/dataTables.bootstrap.min.js")"></script>
    <script src="@Url.ContentCache("~/Scripts/External/dataTables.responsive.min.js")"></script>
    <script src="@Url.ContentCache("~/Scripts/External/responsive.bootstrap.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/External/moment.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/globalization/moment_es.js")"></script>
    <script type="text/javascript">
        var appData = {};
        appData.siteUrl = '@Url.Content("~")';
        appData.formatDate = "@ConfigurationManager.AppSettings["FormatDate"]";
        appData.imgUrl = "@Url.ContentCache("images/loading.gif")";
        appData.imgLoading = $('<img class="ui-box-message-loading" title="Cargando" alt="Cargando" src="' +
            appData.imgUrl +
            '" />');
        appData.registrosPorPagina = "@ConfigurationManager.AppSettings["RegistrosPorPagina"]";
        appData.defaultCulture = "@ConfigurationManager.AppSettings["DefaultCulture"]";
        Globalize.culture(appData.defaultCulture);
    </script>
    @Html.RenderGlobalizationMessagesContent(typeof(CommonStrings))
    <script type="text/javascript" src="@Url.ContentCache("~/Scripts/app.js")"></script>
    @RenderSection("javascript", required: false)
</head>
<body>
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Project name</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a href="#">Home</a></li>
                    <li><a href="#about">About</a></li>
                    <li><a href="#contact">Contact</a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">Dropdown <span class="caret"></span></a>
                        <ul class="dropdown-menu">
                            <li><a href="#">Action</a></li>
                            <li><a href="#">Another action</a></li>
                            <li><a href="#">Something else here</a></li>
                            <li role="separator" class="divider"></li>
                            <li class="dropdown-header">Nav header</li>
                            <li><a href="#">Separated link</a></li>
                            <li><a href="#">One more separated link</a></li>
                        </ul>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="../navbar/">Default</a></li>
                    <li><a href="../navbar-static-top/">Static top</a></li>
                    <li><a href="./">Fixed top <span class="sr-only">(current)</span></a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>
    <div class="container">
        @RenderBody()
    </div>
</body>
</html>
